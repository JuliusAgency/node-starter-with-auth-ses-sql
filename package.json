{
  "name": "@juliusAgency/node-starter-with-auth-ses-sql",
  "version": "1.0.5",
  "description": "Starter for nodejs app",
  "main": "./build/main/index.js",
  "repository": "https://github.com/JuliusAgency/node-starter-with-auth-ses-sql",
  "license": "MIT",
  "keywords": [
    "nodejs",
    "typescript",
    "starter"
  ],
  "scripts": {
    "clean": "./node_modules/.bin/rimraf ./build",
    "build": "run-s clean && npx tsc",
    "dev": "NODE_ENV=dev nodemon ./src/index.ts",
    "start": "NODE_ENV=prod ts-node ./src/index.ts",
    "lint": "run-s lint:*",
    "lint:spell": "cspell \"{README.md,.github/*.md,src/**/*.ts}\"",
    "lint:lint": "eslint src --ext .ts",
    "lint:prettier": "prettier --check \"src/**/*.ts\"",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "test": "jest --detectOpenHandles --coverage --verbose",
    "prepare": "husky install",
    "run": "run-s build && node ./build/main/index.js"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.8.10",
    "@types/passport": "^1.0.14",
    "@types/passport-local": "^1.0.37",
    "@types/pg": "^8.10.9",
    "@types/validator": "^13.11.5",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "cors": "^2.8.5",
    "cspell": "^7.2.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^6.0.0",
    "eslint-plugin-import": "^2.28.1",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.0.2"
  },
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "@juliusagency/auth-mngr": "^1.0.0",
    "@juliusagency/auth-session": "^1.0.2",
    "@juliusagency/auth-strategy-local": "^1.0.1",
    "@juliusagency/auth-user-mngr": "^1.0.0",
    "@juliusagency/auth-utils": "^1.0.0",
    "@juliusagency/auth-verify-service": "^1.0.0",
    "@juliusagency/authorization-repo-sql": "^1.0.1",
    "@juliusagency/authorization-ses-checker": "^1.0.1",
    "@juliusagency/base-user-mngr": "^1.0.1",
    "@juliusagency/base-user-sql": "^1.0.1",
    "@juliusagency/http-logger": "^1.0.1",
    "@juliusagency/simple-email-client": "^1.0.0",
    "@juliusagency/simple-error-handler": "^1.0.1",
    "@juliusagency/simple-logger": "^1.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "passport": "^0.7.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "typeorm": "^0.3.17"
  }
}
